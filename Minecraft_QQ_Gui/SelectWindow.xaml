<Window x:Class="Minecraft_QQ_Gui.SelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minecraft_QQ_Gui"
        mc:Ignorable="d"
        Title="MessageWindow" Height="200" Width="300">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <TextBlock Text="Text" Name="Text" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                   TextWrapping="Wrap" Margin="20"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="50,0,0,15" Height="20" VerticalAlignment="Bottom" Width="58" 
                Foreground="Black" IsDefault="True" Click="Button_Click"/>
        <Button Content="取消" Margin="0,0,50,15" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="58" 
                Foreground="Black" Click="Button_Click_1"/>

    </Grid>
</Window>
