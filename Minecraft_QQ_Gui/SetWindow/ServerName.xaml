<Window x:Class="Minecraft_QQ_Gui.SetWindow.ServerName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
       Title="服务器名字设置" Height="98" Width="220" ResizeMode="NoResize" Background="White" Icon="/Minecraft_QQ.ico" KeyDown="Window_KeyDown">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <Label Content="名字" Margin="6,8,0,0" 
               Style="{DynamicResource Label}" Foreground="Black" Background="#00898989" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox Margin="45,10,10,0" TextWrapping="NoWrap" Text="{Binding Obj.Name}" Foreground="Black" Background="#225EBDD3" Height="20"
                 VerticalAlignment="Top"/>
        <Label Content="别名" Margin="6,31,0,0" 
            Style="{DynamicResource Label}" Foreground="Black" Background="#00898989" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox Margin="45,34,10,0" TextWrapping="NoWrap" Text="{Binding Obj.Addr}" Foreground="Black" Background="#225EBDD3" Height="20" 
                 VerticalAlignment="Top"/>

    </Grid>
</Window>
