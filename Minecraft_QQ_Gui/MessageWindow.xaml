<Window x:Class="Minecraft_QQ_Gui.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minecraft_QQ_Gui"
        mc:Ignorable="d"
        Title="MessageWindow" Height="200" Width="300">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <TextBlock Text="Text" Name="Text" FontSize="20" TextWrapping="Wrap"
                   HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center" Margin="10"/>

    </Grid>
</Window>
