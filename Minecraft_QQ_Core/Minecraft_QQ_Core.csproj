    <Platforms>x64</Platforms>
    <SignAssembly>false</SignAssembly>
    <Version>3.0.0.0</Version>
    <AssemblyName>Minecraft_QQ</AssemblyName>
    <RootNamespace>Minecraft_QQ</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <OutputPath>..\build_out\core</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="img\Minecraft_QQ.jpg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Costura.Fody" Version="4.1.0" />
    <PackageReference Include="Fody" Version="6.2.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="MySql.Data" Version="8.0.21" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="img\Minecraft_QQ.jpg" />
  </ItemGroup>

</Project>
